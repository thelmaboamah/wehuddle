<%= form_for @chatroom, html: { multipart: true } do |f| %>
			
			<%= f.hidden_field :admin, value: current_user.id %>

      <%= f.label :title %>
      <%= f.text_field :title, class: 'form-control' %>

      <%= f.label :description %>
      <%= f.text_area :description, class: 'form-control' %><br>

      <%= f.submit yield(:button_text), class: "btn btn-default" %>
<% end %>