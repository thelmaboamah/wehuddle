<%= form_for @chatroom, html: { multipart: true }, class: "edit_chatroom" do |f| %>
	<%= f.hidden_field :admin, value: current_user.id %>
  <%= f.label :title %><br />
  <%= f.text_field :title %><br />

  <%= f.label :description %><br />
  <%= f.text_area :description%><br />

  <%= f.submit yield(:button_text), class: "btn btn-default" %>

<% end %>
